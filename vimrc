set number
set relativenumber
set clipboard=unnamedplus
set tabstop=4
set shiftwidth=4
"set autoindent"
set smartindent
set cursorline
set mouse=a
set ignorecase
set encoding=utf-8
set bg=dark

colorscheme gruvbox 

so $HOME/configs/vim/plugins.vim
so $HOME/configs/vim/maps.vim
so $HOME/configs/vim/plugin-conf.vim

